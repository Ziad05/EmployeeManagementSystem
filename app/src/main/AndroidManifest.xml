<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.employeemanagementsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".ViewAllActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewAllItemActivity" />
        <activity android:name=".EditActivity" />
        <activity android:name=".ViewEmployeeActivity" />
        <activity
            android:name=".PannelActivity"
            android:label="@string/title_activity_pannel"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".AddDepartment"></activity>
        <activity android:name=".ViewSalaryActivity"></activity>
    </application>

</manifest>